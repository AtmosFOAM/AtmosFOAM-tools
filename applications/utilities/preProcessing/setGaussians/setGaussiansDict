FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setGaussinasDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

backgroundValue T [0 0 0 0 0] 0;
gaussians 1
(
    centre [0 1 0 0 0] (0 0.5 10)
    radius [0 1 0 0 0] 1
    max    [0 0 0 0 0] 1
);

